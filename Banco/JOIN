SELECT * FROM CLIENTE CL

INNER JOIN CONTA CT

ON CL.Id = CT.IdCliente
;


SELECT CL.NOME, CL.SOBRENOME, CT.TIPOCONTA, CT.SALDO FROM CLIENTE CL

INNER JOIN CONTA CT

ON CL.ID = CT.IDCLIENTE

WHERE CL.ANIVERSARIO = '1999-07-13';


SELECT DISTINCT C.NOME FROM CLIENTE C

JOIN CONTA CT

ON C.ID = CT.IDCLIENTE

WHERE CT.SALDO <> 4000;
